<launch>
  <node
    name="master"
    pkg="crazy_ros"
    type="master.py"
    output="screen"
  />
  <node
    name="referenceGenerator"
    pkg="crazy_ros"
    type="referenceGenerator.py"
    output="screen"
    args="$(find crazy_ros)/config/configparam.cnf"
  />
  <node
    name="quadcopterModel"
    pkg="crazy_ros"
    type="quadcopterModel.py"
    output="screen"
    args="$(find crazy_ros)/config/configparam.cnf"
  />
</launch>
